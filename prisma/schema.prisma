// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-kysely"
}

datasource db {
  provider = "sqlite"
  url      = "file:./../dev.db"
}

model Concert {
  id    String @id
  title String
  seats String

  @@map("concerts")
}

model AvailableSeat {
  id         String @id
  concertId  String
  seatNumber Int

  @@map("available_seats")
}
