{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@infra/(.*)$": "<rootDir>/../src/@infra/$1",
    "^@infra": "<rootDir>/../src/@infra"
  }
}
